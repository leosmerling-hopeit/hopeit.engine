{
  "app" : {
    "name": "redis-mon",
    "version": "1.0"
  },
  "engine" : {
    "import_modules": ["hopeit.redis_mon"]
  },
  "env" : {
    "log_reader": {
        "path": "work/logs/apps/",
        "prefix": "simple_example",
        "file_open_timeout_secs": 600,
        "file_checkpoint_expire_secs": 86400,
        "batch_size": 10000,
        "batch_wait_interval_secs": 1,
        "metrics_expire_secs": 3600,
        "aggregate_requests": true,
        "aggregate_events": true
    }
  },
  "events": {
    "log_reader" : {
      "type": "SERVICE",
      "plug_mode": "Standalone"
    },
    "request_status" : {
      "type": "GET",
      "plug_mode": "Standalone"
    }
  }
}
